@Test
    public void getValueAtMultipleTimes_ShouldRetrieveCapacityValues() throws InstantiationException {
        // Arrange
        Timestamp time1 = Timestamp.valueOf("2024-03-01 12:00:00");
        Timestamp time2 = Timestamp.valueOf("2024-03-01 13:00:00");
        Timestamp time3 = Timestamp.valueOf("2024-03-01 14:00:00");

        int capacity1 = 50;
        int capacity2 = 75;
        int capacity3 = 100;

        // Create CAP200Time instance and set capacity measurements
        FactorySensorType factorySensorType = new FactorySensorType();
        Catalogue catalogue = new Catalogue(new PropertyListConfiguration(), factorySensorType);
        catalogue.addSensorType("Capacity", Unit.Percentage);
        Map<Timestamp, Integer> capacityMeasurements = new HashMap<>();

        capacityMeasurements.put(time1, capacity1);
        capacityMeasurements.put(time2, capacity2);
        capacityMeasurements.put(time3, capacity3);
        CAP200Time cap200TimeSensor = new CAP200Time(catalogue);
        cap200TimeSensor.setCapacityMeasurements(capacityMeasurements);

        // Act
        CAP200Value value1 = (CAP200Value) cap200TimeSensor.getValue(time1);
        CAP200Value value2 = (CAP200Value) cap200TimeSensor.getValue(time2);
        CAP200Value value3 = (CAP200Value) cap200TimeSensor.getValue(time3);

        // Assert
        assertEquals(capacity1, value1._dValue);
        assertEquals(capacity2, value2._dValue);
        assertEquals(capacity3, value3._dValue);
    }
}
